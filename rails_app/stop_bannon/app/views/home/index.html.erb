<h1>Stop Steve Bannon!!!!!!!?!?!?!?!?!?</h1>
<table>
  <tr>
    <th>Position</th>
    <th>First Name</th>
    <th>Last Name</th>
    <th>Party</th>
    <th>State</th>
    <th>Denounced Bannon?</th>
    <th>Phone</th>
  </tr>
<% @politicians.each do |data| %>
  <tr>
    <td><%= data.position=='Sen' ? 'Senator' : 'Representative' %></td>
    <td><%= data.first_name %></td>
    <td><%= data.last_name %></td>
    <td><%= data.party=='D' ? 'Democrat' : 'Republican'%> </td>
    <td><%= data.state %></td>
    <td><%= data.denounced ? 'Denounced' : 'Call him/her!'%></td>
    <td><%= data.phone %></td>
  </tr>
<% end %>
</table>
